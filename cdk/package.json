{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk:extension": "cdk --app 'ts-node --prefer-ts-exts ./bin/extension.ts'",
    "cdk:dev": "npm run cdk:extension -- --profile serverless -c env=dev"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.10",
    "aws-cdk": "^2.1004.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.1",
    "typescript": "~5.8.2"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.184.1",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
